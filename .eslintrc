{
  "env": {
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "blockBindings": true,
    "templateStrings": true,
    "octalLiterals": true,
    "binaryLiterals": true,
    "generators": true,
    "forOf": true,
    "objectLiteralShorthandProperties": true
  },
  "rules": {
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-ex-assign": 2,
    "no-reserved-keys": 2,
    "no-unreachable": 2,
    "valid-typeof": 2,
    "no-fallthrough": 2,
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "indent": [
      2,
      2
    ],
    "comma-spacing": 1,
    "semi": 2,
    "space-infix-ops": 2,
    "space-return-throw-case": 2,
    "space-before-function-paren": [
      2,
      "never"
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "new-parens": 2,
    "max-len": [
      2,
      120,
      2
    ],
    "strict": [
      2,
      "global"
    ]
  },
  "globals": {}
}
